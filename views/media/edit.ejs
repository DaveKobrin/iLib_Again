<%- include ('../partials/head.ejs') %> 

<form action="/media/<%= entry.id %>?_method=PUT" method="POST">
    <label for="upc">UPC</label>
    <input type="text" name="upc" id="upc" value="<%= entry.upc %>"/>
    <label for="title">Title</label>
    <input type="text" name="title" id="title" value="<%= entry.title %>"/>
    <label for="creator_name">Creator Name</label>
    <input type="text" name="creator_name" id="creator_name" value="<%= entry.creator_name %>"/>
    <label for="creator_type">Creator Type</label>
    <input type="text" name="creator_type" id="creator_type" value="<%= entry.creator_type %>"/>
    <label for="cover_img">Image Link</label>
    <input type="textarea" name="cover_img" id="cover_img" value="<%= entry.cover_img %>"/>
    <label for="cover_alt">Image Alt Description</label>
    <input type="text" name="cover_alt" id="cover_alt" value="<%= entry.cover_alt %>"/>
    <label for="format">Format</label>
    <input type="text" name="format" id="format" value="<%= entry.format %>"/>
    <label for="location">location</label>
    <input type="text" name="location" id="location" value="<%= entry.location %>"/>
    <label for="tags">Tags</label>
    <input type="text" name="tags" id="tags" value="<%= entry.tags %>"/>
    <label for="desc">Description</label>
    <input type="textarea" name="desc" id="desc" value="<%= entry.desc %>"/>

    <input type="submit" value="Submit"/>
</form>

<%- include ('../partials/foot.ejs') %> 
