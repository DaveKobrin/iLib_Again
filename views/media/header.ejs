<div id="dataProxy" <% if(entries) { %>
    data-entries="<%= JSON.stringify(entries) %>" 
    <% } %>>
</div> 
<label class="form-label" for="format">Format </label>
<select class="form-control w-25" name="format" id="format">
    <option value="All">All</option>
    <% let opt=''; %>
    <% if(dropdownData) { %>
        <% for ( const el of dropdownData.format ) { %>
            <option value="<%= el %>"><%= el %></option>
    <% }} %>  
</select>
